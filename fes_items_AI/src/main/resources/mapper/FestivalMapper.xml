<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.mapper.FestivalMapper">
     
 <select id="findAll" resultType="com.example.app.domain.Festival">
    SELECT * FROM festival
</select>

<select id="findById" resultType="com.example.app.domain.Festival" parameterType="long">
    SELECT * FROM festival WHERE id = #{id}
</select>

</mapper>

